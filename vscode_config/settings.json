                {
                    "key": "s",
                    "name": "status",
                    "type": "command",
                    "command": "workbench.view.scm"
                },
                {
                    "key": "l",
                    "name": "log",
                    "type": "command",
                    "command": "git.viewHistory"
                },
                {
                    "key": "B",
                    "name": "blame_buffer",
                    "type": "command",
                    "command": "gitlens.toggleFileBlame"
                },
                {
                    "key": "d",
                    "name": "diff",
                    "type": "command",
                    "command": "workbench.view.scm"
                },
                {
                    "key": "D",
                    "name": "file_history",
                    "type": "command",
                    "command": "gitlens.viewFileHistory"
                },
                {
                    "key": "g",
                    "name": "+Open",
                    "type": "bindings",
                    "bindings": [
                        {
                            "key": "c",
                            "name": "commit",
                            "type": "command",
                            "command": "gitlens.openCommitOnRemote"
                        },
                        {
                            "key": "p",
                            "name": "pr",
                            "type": "command",
                            "command": "gitlens.openAssociatedPullRequestOnRemote"
                        }
                    ]
                }
            ]
        },
        {
            "key": "l",
            "name": "+LSP",
            "type": "bindings",
            "command": [
                {
                    "key": "s",
                    "name": "format",
                    "type": "command",
                    "command": "editor.action.formatDocument"
                }
            ]
        },
        {
            "key": "r",
            "name": "+Refactor",
            "type": "bindings",
            "bindings": [
                {
                    "key": "/",
                    "name": "local",
                    "type": "command",
                    "command": "search.action.replaceAllInFile"
