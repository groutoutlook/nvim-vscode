                    "key": "g",
                    "name": "+Git",
                    "type": "bindings",
                    "bindings": [
                        {
                            "key": "s",
                            "name": "status",
                            "type": "command",
                            "command": "gitlens.gitCommands.status"
                        },
                        {
                            "key": "b",
                            "name": "branches",
                            "type": "command",
                            "command": "git.checkout"
                        },
                        {
                            "key": "c",
                            "name": "commits",
                            "type": "command",
                            "command": "gitlens.showCommitSearch"
                        },
                        {
                            "key": "z",
                            "name": "stashes",
                            "type": "command",
                            "command": "gitlens.gitCommands.stash.list"
                        }
                    ]
                },
