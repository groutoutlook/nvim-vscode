                {
                    "key": "?",
                    "name": "global",
                    "type": "command",
                    "command": "workbench.action.replaceInFiles"
                },
                {
                    "key": "n",
                    "name": "rename",
                    "type": "command",
                    "command": "editor.action.refactor"
                },
                {
                    "key": "r",
                    "name": "refactor",
                    "type": "command",
                    "command": "editor.action.refactor"
